{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "mv -f ./src/config-testnet.ts ./src/config.ts && PORT=3003 npm run start:dev",
    "prod": "mv -f ./src/config-prod.ts ./src/config.ts && npm start",
    "start:dev": "rollup -c -w",
    "start": "sirv public --host -p 5000",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/es6-promisify": "^6.0.0",
    "@types/node": "^14.14.10",
    "@types/socket.io-client": "^1.4.34",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-hash-router": "^1.0.1",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "lamden_wallet_controller": "^0.6.3",
    "lamden-js": "^1.4.9",
    "sirv-cli": "^1.0.0",
    "socket.io-client": "^2.3"
  }
}
